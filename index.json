{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "printWidth": 100,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],

  "importOrder": ["^@angular/(.*)$", "^react(.*)$", "<THIRD_PARTY_MODULES>", "^../(.*)$", "^./(.*)$"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["decorators", "typescript"]
}
